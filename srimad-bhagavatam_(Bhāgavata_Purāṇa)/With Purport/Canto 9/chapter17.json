        "verse": "1-3",
        "devanagari": "\u0936\u094d\u0930\u0940\u092c\u093e\u0926\u0930\u093e\u092f\u0923\u093f\u0930\u0941\u0935\u093e\u091a\u092f: \u092a\u0941\u0930\u0942\u0930\u0935\u0938: \u092a\u0941\u0924\u094d\u0930 \u0906\u092f\u0941\u0938\u094d\u0924\u0938\u094d\u092f\u093e\u092d\u0935\u0928\u094d \u0938\u0941\u0924\u093e: \u0964\u0928\u0939\u0941\u0937: \u0915\u094d\u0937\u0924\u094d\u0930\u0935\u0943\u0926\u094d\u0927\u0936\u094d\u091a \u0930\u091c\u0940 \u0930\u093e\u092d\u0936\u094d\u091a \u0935\u0940\u0930\u094d\u092f\u0935\u093e\u0928\u094d \u0965 \u0967 \u0965\u0905\u0928\u0947\u0928\u093e \u0907\u0924\u093f \u0930\u093e\u091c\u0947\u0928\u094d\u0926\u094d\u0930 \u0936\u200d\u0943\u0923\u0941 \u0915\u094d\u0937\u0924\u094d\u0930\u0935\u0943\u0927\u094b\u093d\u0928\u094d\u0935\u092f\u092e\u094d \u0964\u0915\u094d\u0937\u0924\u094d\u0930\u0935\u0943\u0926\u094d\u0927\u0938\u0941\u0924\u0938\u094d\u092f\u093e\u0938\u0928\u094d \u0938\u0941\u0939\u094b\u0924\u094d\u0930\u0938\u094d\u092f\u093e\u0924\u094d\u092e\u091c\u093e\u0938\u094d\u0924\u094d\u0930\u092f: \u0965 \u0968 \u0965\u0915\u093e\u0936\u094d\u092f: \u0915\u0941\u0936\u094b \u0917\u0943\u0924\u094d\u0938\u092e\u0926 \u0907\u0924\u093f \u0917\u0943\u0924\u094d\u0938\u092e\u0926\u093e\u0926\u092d\u0942\u0924\u094d \u0964\u0936\u0941\u0928\u0915: \u0936\u094c\u0928\u0915\u094b \u092f\u0938\u094d\u092f \u092c\u0939\u094d\u0935\u0943\u091a\u092a\u094d\u0930\u0935\u0930\u094b \u092e\u0941\u0928\u093f: \u0965 \u0969 \u0965",
